    "@types/react-dom": "^18.2.11",
    "@types/webextension-polyfill": "^0.10.7",
    "clsx": "^2.1.1",
    "esbuild": "^0.19.5",
    "p-queue": "^7.3.4",
    "react": "^18.2.0",
    "react-controlled-translation": "^1.0.20",
    "react-dom": "^18.2.0",
    "string-width": "^7.2.0",
    "tsx": "^4.6.2",
    "webextension-polyfill": "^0.10.0"
  }
}