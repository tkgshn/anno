{
  "name": "safari-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsx build.ts",
    "build:watch": "tsx watch build.ts",
    "convert": "xcrun safari-web-extension-converter --bundle-identifier jp.anno.safari --force dist",
